{
  "name": "@openharmony/dlp_permission_service",
  "description": "dlp_permission_service",
  "version": "3.1.0",
  "license": "Apache License 2.0",
  "publishAs": "code-segment",
  "segment": {
    "destPath": "base/security/dlp_permission_service"
  },
  "dirs": {},
  "scripts": {},
  "component": {
    "name": "dlp_permission_service",
    "subsystem": "security",
    "syscap": [
      "SystemCapability.Security.DlpPermissionService"
    ],
    "hisysevent_config": [
      "//base/security/dlp_permission_service/hisysevent.yaml"
    ],
    "features": [
      "dlp_permission_service_feature_coverage",
      "dlp_permission_service_gathering_policy"
    ],
    "adapted_system_type": [
      "standard"
    ],
    "rom": "2048KB",
    "ram": "5102KB",
    "deps": {
      "components": [
        "hitrace_native",
        "ipc",
        "os_account",
        "access_token",
        "bundle_framework",
        "file_api",
        "dlp_credential_service",
        "common_event_service",
        "init",
        "ability_base",
        "ability_runtime",
        "hiviewdfx_hilog_native",
        "hisysevent_native",
        "napi",
        "safwk",
        "samgr",
        "c_utils",
        "utils_base"
      ],
      "third_party": [
        "libfuse",
        "openssl"
      ]
    },
    "build": {
      "sub_component": [
        "//base/security/dlp_permission_service:dlp_permission_build_module"
      ],
      "inner_kits": [
        {
          "name": "//base/security/dlp_permission_service/interfaces/inner_api/dlp_permission:libdlp_permission_sdk",
          "header": {
            "header_files": [
              "dlp_permission_kit.h"
            ],
            "header_base": "//base/security/dlp_permission_service/interfaces/inner_api/dlp_permission/include"
          }
        },
        {
          "name": "//base/security/dlp_permission_service/interfaces/inner_api/dlp_parse:libdlpparse",
          "header": {
            "header_files": [
              "dlp_crypt.h",
              "dlp_file_kits.h"
            ],
            "header_base": "//base/security/dlp_permission_service/interfaces/inner_api/dlp_parse/include"
          }
        }

      ],
      "test": [
        "//base/security/dlp_permission_service:dlp_permission_build_module_test",
        "//base/security/dlp_permission_service/test/fuzztest/dlp_permission:fuzztest",
        "//base/security/dlp_permission_service/interfaces/inner_api/dlp_permission/test:dlp_permission_test",
        "//base/security/dlp_permission_service/interfaces/inner_api/dlp_parse/test:dlp_parse_test",
        "//base/security/dlp_permission_service/interfaces/inner_api/dlp_fuse/test:dlp_fuse_test"
      ]
    }
  }
}
